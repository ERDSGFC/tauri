{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "types": ["element-plus/global"],
        "lib": ["ESNext", "DOM"],
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@css*": ["src/assets/css*"],
            "@icon*": ["src/assets/icon/*"],
            "@image*": ["src/assets/image/*"]

        }
    },
    "exclude": [
        "node_modules","images","src-tauri", "dist"
    ]
}